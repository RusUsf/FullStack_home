{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MonkeyDB": "User ID=burner;Password=burner;Host=localhost;Port=5432;Database=monkey_db;Search Path=public;CommandTimeout =120;"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.PostgreSQL" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "PostgreSQL",
        "Args": {
          "connectionString": "User ID=burner;Password=burner;Host=localhost;Port=5432;Database=monkey_db;Search Path=public;CommandTimeout =120;",
          "tableName": "\"Logs\"",
          "autoCreateSqlTable": true,
          "restrictedToMinimumLevel": "Information",
          "batchPostingLimit": 1000,
          "period": "0.00:00:30",
          "formatProvider": "none",
          "columnOptionsSection": {
            "customColumns": [
              {
                "ColumnName": "EventType",
                "DataType": "integer",
                "AllowNull": false
              },
              {
                "ColumnName": "Release",
                "DataType": "varchar",
                "DataLength": 32
              },
              {
                "ColumnName": "Parameters",
                "DataType": "jsonb",
                "AllowNull": true
              }
            ]
          }
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Your application name"
    }
  }
}
